<div class="flex flex-col items-center w-full p-4 overflow-scroll">
  <p class="mt-6 text-3xl leading-8 text-gray-600">Hello <%= "#{current_user.first_name}!" %></p>
  <div id="date"></div>
  <div id="clock"></div>
  <%= image_tag("5498791.jpg", class: "object-contain h-52 w-96") %>
  <button type="button" class="inline-flex items-center rounded-md border border-transparent bg-yellow-400 px-6 py-3 text-base font-medium text-white shadow-sm hover:bg-yellow-300 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-offset-2">Add Member</button>

  <%= button_to "Clock In", user_time_in_path %>
  <%= button_to "Clock Out", user_time_out_path %>
</div>
